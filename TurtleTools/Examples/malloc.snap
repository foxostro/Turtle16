import stdlib

struct Point2 {
	x: u16,
	y: u16
}

impl Point2 {
	func clone(self: *Point2) -> *Point2 {
		let point: *Point2 = malloc()
		point.x = self.x
		point.y = self.y
		return point
	}
}

test "Clone on the heap" {
	let p1 = Point2 { .x = 100, .y = 200 }
	let p2 = p1.clone()
	assert(p1.x == p2.x)
	assert(p1.y == p2.y)
}
